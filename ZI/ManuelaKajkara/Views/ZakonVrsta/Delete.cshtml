@model RazinaZakona

@{
    ViewBag.Title = "Brisanje razine zakona";
}

<h2>Brisanje</h2>

<h3>Jeste li sigurni da želite obrisati ovu razinu zakona?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.LabelForModel("Naziv razine", new { @class = "control-label col-md-2" })
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Naziv)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Obriši" class="btn btn-default" /> |
            @Html.ActionLink("Povratak", "Index")
        </div>
    }
</div>
