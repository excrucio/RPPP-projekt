@model IEnumerable<VrstaOznake>

@{
    ViewBag.Title = "Vrste oznaka";
}

<h2>Vrste oznake</h2>

<p>
    @Html.ActionLink("Unesi novu", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SifraOznake)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Opis)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SifraOznake)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Opis)
        </td>
        <td>
            @Html.ActionLink("Uredi", "Edit", new { id=item.SifraOznake }) |
            @Html.ActionLink("Detalji", "Details", new { id=item.SifraOznake }) |
            @Html.ActionLink("Brisi", "Delete", new { id=item.SifraOznake })
        </td>
    </tr>
}

</table>
