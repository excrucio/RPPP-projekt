@model DAL.EF.TipPresude

@{
    ViewBag.Title = "Obriši";
}

<h2>Orbiši</h2>

<h3>Sigurno?</h3>
<div>
    <h4>Tip presude</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Naziv tipa
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NazivTipa)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Obriši" class="btn btn-default" /> |
            @Html.ActionLink("Povratak", "Index")
        </div>
    }
</div>
