                @Html.EditorFor(model => model.IDStavke)
                @Html.ValidationMessageFor(model => model.IDStavke)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Dokument, new { @class = "control-label col-md-2" })
